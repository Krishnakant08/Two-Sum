  vector<int> twoSum(vector<int>& nums, int target) {
        

        vector<int>A;
        int n=nums.size();

        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++){
                if(nums[i]+nums[j]==target){
                    A.push_back(i);
                    A.push_back(j);
                }
            }
        }
        return A;
    }
